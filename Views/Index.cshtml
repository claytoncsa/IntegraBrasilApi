@{
    // Obtém os dados do objeto de modelo
    var modelo = Model;
}

<h1>Endereço</h1>

<table>
    <tr>
        <th>CEP</th>
        <td>@modelo.Cep</td>
    </tr>
    <tr>
        <th>Logradouro</th>
        <td>@modelo.Logradouro</td>
    </tr>
    <tr>
        <th>Bairro</th>
        <td>@modelo.Bairro</td>
    </tr>
    <tr>
        <th>Cidade</th>
        <td>@modelo.Cidade</td>
    </tr>
    <tr>
        <th>Estado</th>
        <td>@modelo.Estado</td>
    </tr>
</table>
